{
  "name": "angularnodesearch",
  "version": "1.0.0",
  "description": "Search Service",
  "main": "build/server.js",
  "scripts": {
    "serve": "node build/server.js",
    "watch-serve": "nodemon build/server.js",
    "build": "tsc --project ./",
    "watch-build": "tsc -w",
    "start": "npm run build && npm run serve",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-build\" \"npm run watch-serve\"",
    "lint": "eslint --fix",
    "test": "nyc --reporter=lcov mocha -r ts-node/register src/tests/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.13",
    "@types/sinon": "^9.0.9",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "sinon": "^9.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "pg-promise": "^10.8.6",
    "tslog": "^3.0.2",
    "validator": "^13.5.2"
  }
}
